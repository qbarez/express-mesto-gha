{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "plugins": [
    "import"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  }
}
